        android:id="@+id/btn0"
        android:layout_width="76.6dp"
        android:layout_height="46dp"
        android:layout_marginTop="5dp"
        android:background="#434343"
        android:text="0"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
