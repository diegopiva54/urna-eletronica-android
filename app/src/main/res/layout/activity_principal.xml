        app:layout_constraintTop_toBottomOf="@+id/btn0" />

    <Button
        android:id="@+id/btnCorrige"
        android:layout_width="76.6dp"
        android:layout_height="35dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="5dp"
        android:background="#FF9800"
        android:text="CORRIGE"
        android:textColor="#252525"
        android:textColorHighlight="#2E2E2E"
        android:textColorHint="#323131"
        android:textSize="13dp"
        app:layout_constraintBottom_toBottomOf="@+id/view2"
        app:layout_constraintEnd_toStartOf="@+id/btnConfirma"
        app:layout_constraintTop_toBottomOf="@+id/btn0" />

    <Button
        android:id="@+id/btnConfirma"
        android:layout_width="76.6dp"
        android:layout_height="35dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="5dp"
        android:background="#4CAF50"
        android:text="CONFIRMA"
        android:textColor="#252525"
        android:textColorHighlight="#2E2E2E"
        android:textColorHint="#323131"
        android:textSize="13dp"
        app:layout_constraintBottom_toBottomOf="@+id/view2"
        app:layout_constraintEnd_toEndOf="@+id/view2"
        app:layout_constraintTop_toBottomOf="@+id/btn0" />

    <Button
        android:id="@+id/btn9"
        android:layout_width="76.6dp"
        android:layout_height="46dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="5dp"
        android:background="#434343"
        android:text="9"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/view2"
        app:layout_constraintTop_toBottomOf="@+id/btn6" />

    <Button
        android:id="@+id/btn6"
        android:layout_width="76.6dp"
        android:layout_height="46dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="5dp"
